{
    "name": "ashraam/apimo",
    "description": "Package compatible Laravel pour se connecter Ã  la solution Apimo",
    "keywords": [
        "ashraam",
        "apimo"
    ],
    "homepage": "https://github.com/ashraam/apimo",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Romain BERTOLUCCI",
            "email": "romain.bertolucci@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ashraam\\Apimo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ashraam\\Apimo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ashraam\\Apimo\\ApimoServiceProvider"
            ],
            "aliases": {
                "Apimo": "Ashraam\\Apimo\\ApimoFacade"
            }
        }
    }
}
